<?xml version="1.0" encoding="UTF-8"?>
<!-- Settings.xml is not read by default, it is part of systems. -->
<settings>
	
	<!-- Application - basic app settings -->
	<application>
		
		<!-- Window Settings -->
		<window>
			<width>1280</width>
			<height>720</height>
			<fullscreen>false</fullscreen>
			<title>Wired</title>
		</window>
		
		<!-- Renderer Settings -->
		<renderer>
			<width>1280</width>
			<height>720</height>
			
			<v_sync>true</v_sync>
			
			<depth_bits>24</depth_bits>
			
			<color_buffer>
				<red_bits>3</red_bits>
				<green_bits>3</green_bits>
				<blue_bits>2</blue_bits>
				<alpha_bits>0</alpha_bits>
			</color_buffer>
		</renderer>
		
	</application>
	
	
	<!-- Asset Directories - directories for loading etc. -->
	<systems>
		

		
	</systems>

</settings>
