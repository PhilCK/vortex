<?xml version="1.0" encoding="UTF-8"?>

<!-- Level -->

<entity>
	
	<name>Level</name>
	<tags>level</tags>

	<components>
		
		<!-- Transform -->
		
		<component>
			<name>Transform</name>
			<params>
				<!--<scale>0.5 0.5 0.5</scale>-->
			</params>
		</component>

		<component>
			<name>Model</name>
			<params>
				<model_name>LevelGeo01.obj</model_name>
			</params>
		</component>

		<component>
			<name>BlinnPhongMaterial</name>
			<params>
				<rgba>1 1 0 1</rgba>
				<diffuse_map>dev_grid_blue.png</diffuse_map>
			</params>
		</component>	

		<!-- MVC -->
		
		<component>
			<name>LevelModel</name>
		</component>
		
		<component>
			<name>LevelView</name>
			<params>
				<outter_one>
					<model_name>LevelGeo01.obj</model_name>
					<turn_speed>0.4f</turn_speed>
				</outter_one>
			</params>
		</component>

	</components>
		
</entity>
